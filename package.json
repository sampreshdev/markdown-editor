{
  "name": "antrika-customer-portal",
  "description": "Trackwick React front end",
  "version": "1.0.1",
  "scripts": {
    "dev-vm": "vite --port 8080",
    "server": "node server.js",
    "storybook": "start-storybook",
    "deploy": "npm run build && npm run server",
    "release-dry-run": "standard-version --dry-run",
    "prepare": "husky install",
    "start": "vite --mode production --port 8080",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "format": "prettier \"**/*.{js,jsx,json,md,scss,yaml,yml}\" --write",
    "release": "standard-version",
    "preview": "vite preview"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "commitUrlFormat": "https://gitlab.com/trackolap/antrika-customer-portal/commits/{{hash}}",
    "compareUrlFormat": "https://gitlab.com/trackolap/antrika-customer-portal/compare/{{currentTag}}%0D{{previousTag}}"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "Firefox ESR"
  ],
  "engines": {
    "node": "^12.0",
    "npm": "^6.0"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.0",
    "@ant-design/icons": "^4.0.0",
    "@codexteam/ajax": "^4.2.0",
    "@editorjs/code": "^2.9.0",
    "@editorjs/delimiter": "^1.4.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/header": "^2.8.1",
    "@editorjs/link": "^2.6.2",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/nested-list": "^1.4.2",
    "@editorjs/paragraph": "^2.11.3",
    "@editorjs/table": "^2.3.0",
    "@editorjs/underline": "^1.1.0",
    "@egg-/editor-js-quote": "^2.5.0",
    "@egg-/editorjs-drag-drop": "^1.1.13",
    "@sentry/react": "^5.19.2",
    "antd": "^5.10.1",
    "axios": "^1.6.7",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.10",
    "editorjs-drag-drop": "^1.1.13",
    "editorjs-inline-font-size-tool": "^1.0.1",
    "editorjs-text-color-plugin": "^2.0.4",
    "editorjs-tooltip": "^1.1.15",
    "editorjs-undo": "^2.0.26",
    "jodit-react": "^1.3.39",
    "promise-polyfill": "^8.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-editor-js": "^2.1.0",
    "react-final-form": "^6.5.9",
    "react-ga4": "^2.1.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "customize-cra": "^0.9.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "git-branch-is": "^3.0.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.1",
    "standard-version": "^9.5.0",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/trackolap/antrika-customer-portal.git"
  },
  "author": "Trackolap",
  "homepage": "https://gitlab.com/trackolap/antrika-customer-portal#readme",
  "license": "ISC",
  "type": "module",
  "private": true
}
